{
  "uid": "code-agent",
  "title": "Code Agent",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "JVM Heap Used",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "jvm_memory_used_bytes{application=\"code-agent\",area=\"heap\"}",
          "legendFormat": "{{id}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "HTTP Request Rate",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "rate(http_server_requests_seconds_count{application=\"code-agent\"}[1m])",
          "legendFormat": "{{method}} {{uri}} {{status}}",
          "refId": "A"
        }
      ]
    }
  ]
}
